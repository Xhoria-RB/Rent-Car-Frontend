{  
  "extends": "airbnb",  
  "parser": "babel-eslint",  
  "env": 
    {    
      "node": true,    
      "es6": true,    
      "browser": true  
    },  
  "rules": 
    {    
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],    
      "implicit-arrow-linebreak": "off",    
      "comma-dangle": "off",
      "react/jsx-one-expression-per-line": "off",
      "brace-style": ["error", "stroustrup"],
      "react/prop-types": ["warn"],
      "import/named": "off"
    }
  }
